{
  "name": "funfunzmc",
  "version": "0.0.6",
  "description": "backoffice generator",
  "main": "lib/api/index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint src/*.js",
    "dev": "DEBUG=funfunzmc:* NODE_ENV=development babel-node --extensions \".js,.ts,.tsx,.json\" ./dev.ts",
    "questions": "DEBUG=funfunzmc:* NODE_ENV=development babel-node --extensions \".js,.ts,.tsx,.json\" ./src/index.ts",
    "clean": "rm -rf lib bin && mkdir lib",
    "type-check": "tsc",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run clean && npm run build:types && npm run build:js",
    "build:types": "tsc",
    "build:js": "babel src -d lib --extensions \".js,.ts,.tsx\" --source-maps inline && cp -rf ./src/api/public/* ./lib/api/public",
    "start": "NODE_ENV=production node ./lib/index.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JWebCoder/funfunzmc.git"
  },
  "keywords": [
    "backoffice",
    "generator"
  ],
  "author": "Jo√£o Moura",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JWebCoder/funfunzmc/issues"
  },
  "homepage": "https://github.com/JWebCoder/funfunzmc#readme",
  "dependencies": {
    "@types/mysql2": "github:types/mysql2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "enquirer": "^2.3.0",
    "express": "^4.16.4",
    "express-fileupload": "^1.1.1-alpha.1",
    "jsonschema": "^1.2.4",
    "knex": "^0.16.3",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.4",
    "pluralize": "^7.0.0",
    "tslint": "^5.12.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/debug": "^4.1.0",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.12.21",
    "@types/pluralize": "0.0.29",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-module-resolver": "^3.1.3",
    "eslint": "^5.13.0",
    "typescript": "^3.3.3"
  }
}
