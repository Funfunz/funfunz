{
  "name": "funfunz",
  "version": "0.2.1",
  "description": "backoffice generator",
  "main": "lib/api/index.js",
  "engines": {
    "node": ">=9.11.1"
  },
  "scripts": {
    "test": "jest --coverage --forceExit && codecov",
    "lint": "tslint src/**/*.ts",
    "dev": "DEBUG=funfunz:* NODE_ENV=development ts-node ./src/example/index.ts",
    "questions": "DEBUG=funfunz:* NODE_ENV=development ts-node ./src/generator/index.ts",
    "clean": "rm -rf lib && mkdir lib",
    "build": "npm run lint && npm run clean && npm run build:js",
    "build:js": "tsc",
    "start": "NODE_ENV=production node ./lib/index.js",
    "prepublishOnly": "npm run build",
    "docker-compose:up": "docker-compose up -d",
    "docker-compose:down": "docker-compose down --remove-orphans",
    "mysql:shell": "docker-compose exec mysqldb bash -c 'mysql -u root -p$MYSQL_ROOT_PASSWORD $MYSQL_DATABASE'"
  },
  "bin": {
    "funfunz": "./lib/generator/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JWebCoder/funfunz.git"
  },
  "keywords": [
    "backoffice",
    "generator"
  ],
  "author": "Jo√£o Moura",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JWebCoder/funfunz/issues"
  },
  "homepage": "https://github.com/JWebCoder/funfunz#readme",
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/mongodb": "^3.5.27",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.11.3",
    "@types/pluralize": "0.0.29",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint": "^7.10.0",
    "jest": "^26.5.0",
    "supertest": "^5.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-graphql": "^0.11.0",
    "graphql": "^15.3.0",
    "graphql-parse-resolve-info": "^4.9.0",
    "jsonschema": "^1.2.8",
    "morgan": "^1.10.0",
    "pluralize": "^8.0.0"
  },
  "optionalDependencies": {
    "knex": "^0.21.6",
    "mongodb": "^3.6.2",
    "mysql2": "^2.2.5"
  }
}
